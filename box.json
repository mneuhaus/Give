{
    "alias": "give.phar",
    "chmod": "0755",
    "directories": ["src"],
    "files": [
        "vendor/herrera-io/phar-update/res/schema.json"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["Tests"],
            "in": "vendor"
        }
    ],
    "main": "bin/main",
    "git-version": "git_tag",
    "output": "Repository/give-@git-version@.phar",
    "replacements": {
        "manifest_url": "https://raw.github.com/mneuhaus/Give-Repository/master/manifest.json"
    },
    "stub": true
}